{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.git": true,
    "**/node_modules": true,
    "**/.vscode": true,
    "Frontend": true
  },
  "explorerExclude.backup": {},
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "white",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "fireBrick",
      "bold": false,
      "italic": false
    },
    {
