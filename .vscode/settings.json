      "strikethrough": false,
      "underline": false,
      "backgroundColor": "mediumSlateBlue",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "white",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "darkOliveGreen",
      "bold": false,
      "italic": false
    },
    {
